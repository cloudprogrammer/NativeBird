{
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": [
        0
      ],
      "react/require-default-props": [
        0
      ],
      "global-require": [
        0
      ]
    }
  }